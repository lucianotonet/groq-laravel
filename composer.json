{
    "name": "lucianotonet/groq-laravel",
    "description": "Laravel package to access Groq REST API",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Luciano Tonet",
            "email": "tonetlds@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "lucianotonet/groq-php": "^1.0.0",
        "illuminate/support": "*",
        "illuminate/contracts": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LucianoTonet\\GroqLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucianoTonet\\GroqLaravel\\GroqServiceProvider"
            ],
            "aliases": {
                "Groq": "LucianoTonet\\GroqLaravel\\Facades\\Groq"
            }
        }
    }
}
